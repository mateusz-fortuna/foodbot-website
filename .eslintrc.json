{
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "prettier", "eslint:recommended", "plugin:react-hooks/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 0,
    "semi": ["error", "always"],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  }
}
